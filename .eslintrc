{
  "rules": {
    "indent": [
      2,
      2, {
        "SwitchCase": 1
      }
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-console": 0,
    "no-unsafe-finally": "off"
  },
  "env": {
    "es6": true,
    "browser": true
  },
  parserOptions: {
    ecmaVersion: 6,
    sourceType: "module"
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true
  },
  "globals": {
    "Modernizr": true,
    "require": false
  }
}
